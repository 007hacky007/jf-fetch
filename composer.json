{
    "name": "jf-fetch/app",
    "description": "JF Fetch web application",
    "type": "project",
    "require": {
        "php": "^8.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
